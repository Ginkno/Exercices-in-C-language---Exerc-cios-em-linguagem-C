#include <iostream>
#include <iomanip>
#include <climits>
#include <cmath>

using namespace std;

int main() 
{
  int a, b, c, menor;

  cout << "Digite trÃªs numeros" << endl;
  cout << "Primeiro: ";
  cin >> a;
  cout << "Segundo: ";
  cin >> b;
  cout << "Terceiro: ";
  cin >> c;

  if (a < b && a < c) {
    menor = a;
  } else if (b < c && b < a) {
    menor = b;
  } else {
    menor = c;
  }

  cout << "MENOR NUMERO: " << menor << endl;
} 
